<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    
     <managed-bean>
<managed-bean-name>navegacion</managed-bean-name>
<managed-bean-class>gt.gob.segeplan.sisag.core.sisag.web.managed.NavegacionManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
<managed-bean-name>seguridad</managed-bean-name>
<managed-bean-class>gt.gob.segeplan.sisag.core.sisag.web.managed.seguridadManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    
    <managed-bean>
<managed-bean-name>aplicationBean</managed-bean-name>
<managed-bean-class>gt.gob.segeplan.sisag.core.sisag.web.utils.aplicationBean</managed-bean-class>
<managed-bean-scope>application</managed-bean-scope>
    </managed-bean>

    <managed-bean>
<managed-bean-name>sessionBean</managed-bean-name>
<managed-bean-class>gt.gob.segeplan.sisag.core.sisag.web.utils.sessionBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <application>
        
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
       
    </application>
</faces-config>
